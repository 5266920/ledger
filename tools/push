#!/bin/sh

set -e

(cd plan/data; git push)
(cd plan; git commit -a -m "Update TODO files" && git push)
git checkout master
git merge --no-ff next
git checkout next
git rebase master
git push
git checkout master
./acprep --enable-cache --universal -j16 --warn opt upload
./acprep --enable-cache --universal -j16 --warn opt make speedtest | \
    tee build/last-speed.txt
mv *.dmg* build
git checkout next
