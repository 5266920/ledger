#!/bin/sh

./ledger -f test.dat entry 10/20 "stock option"
./ledger -f test.dat entry 10/20 "stock option" -20
./ledger -f test.dat entry 10/20 "stock option" opti
./ledger -f test.dat entry 10/20 "stock option" time
./ledger -f test.dat entry 10/20 "stock option" gain
./ledger -f test.dat entry 10/20 "stock option" opti -20
./ledger -f test.dat entry 10/20 "stock option" time -20
./ledger -f test.dat entry 10/20 "stock option" gain -20

./ledger -f test.dat entry 10/20 "stock optionx"
./ledger -f test.dat entry 10/20 "stock optionx" -20
./ledger -f test.dat entry 10/20 "stock optionx" opti
./ledger -f test.dat entry 10/20 "stock optionx" time
./ledger -f test.dat entry 10/20 "stock optionx" gain
./ledger -f test.dat entry 10/20 "stock optionx" opti -20
./ledger -f test.dat entry 10/20 "stock optionx" time -20
./ledger -f test.dat entry 10/20 "stock optionx" gain -20

./ledger -f test.dat entry 10/20 "stock option" xopti
./ledger -f test.dat entry 10/20 "stock option" xtime
./ledger -f test.dat entry 10/20 "stock option" xgain
./ledger -f test.dat entry 10/20 "stock option" xopti -20
./ledger -f test.dat entry 10/20 "stock option" xtime -20
./ledger -f test.dat entry 10/20 "stock option" xgain -20

./ledger -f test.dat entry 10/20 "stock optionx" xopti
./ledger -f test.dat entry 10/20 "stock optionx" xtime
./ledger -f test.dat entry 10/20 "stock optionx" xgain
./ledger -f test.dat entry 10/20 "stock optionx" xopti -20
./ledger -f test.dat entry 10/20 "stock optionx" xtime -20
./ledger -f test.dat entry 10/20 "stock optionx" xgain -20
